{% with width=width|default:"32px" height=height|default:"32px" %}
  <div>
    <style>
    me {
      display: flex;
      align-items: center;
      border-radius: 100%;
      overflow: hidden;
      width: {{ width }};
      height: {{ height }};
      background-color: {{ logo.bg_color }};
    }
    me img {
      display: block;
      margin: 0 auto;
      max-width: 80%;
      max-height: 80%;
    }
    </style>
    <img src="{{ logo.url }}" alt="TODO alt" />
  </div>
{% endwith %}
